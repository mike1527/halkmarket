<div class="content__inner">
    <div class="content__inner-box">
        <div class="content__body">
            {{ header }}

            <script src="catalog/view/javascript/jquery/jquery-3.5.1.min.js" type="text/javascript"></script>
            <script src="catalog/view/javascript/common.js" type="text/javascript"></script>

            <link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />

            {% for style in styles %}
                <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
            {% endfor %}
                {% for link in links %}
                    <link href="{{ link.href }}" rel="{{ link.rel }}" />
                {% endfor %}
                {% for analytic in analytics %}
                    {{ analytic }}
                {% endfor %}



            <div class="container">
              <ul class="breadcrumb breadcrumb__list">
                {% for key, breadcrumb in breadcrumbs %}
                  {% if key + 1 < breadcrumbs|length %}
                    <li class="breadcrumb__item breadcrumb__item-hover"><a class="breadcrumb__link" href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                  {% else %}
                    <li class="breadcrumb__item">{{ text_breadcrumps }}</li>
                  {% endif %}
                {% endfor %}
              </ul>

              <section class="infodelivery">
                  <h1 class="infodelivery__title">{{ text_title }}</h1>
                  <div class="infodelivery__wrapper">
                      <div class="infodelivery__item">
                          <h3 class="infodelivery__item-title">{{ text_content_title1 }}</h3>
                          <div class="infodelivery__box">
                              <ol class="infodelivery__list infodelivery__list-first">
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_1 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_2 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_3 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_4 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_5 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_6 }}</li>
                                  <li class="infodelivery__list-item list-item__first">{{ text_content_1_7 }}</li>
                              </ol>
                          </div>
                          <h3 class="infodelivery__item-title">{{ text_content_title2 }}</h3>
                          <div class="infodelivery__box infodelivery__box-second">
                              <ol class="infodelivery__list infodelivery__list-second">
                                  <li class="infodelivery__list-item list-item__second">{{ text_content_2_1 }}</li>
                                  <li class="infodelivery__list-item list-item__second">{{ text_content_2_2 }}</li>
                                  <li class="infodelivery__list-item list-item__second">{{ text_content_2_3 }}</li>
                                  <li class="infodelivery__list-item list-item__second">{{ text_content_2_4 }}</li>
                                  <li class="infodelivery__list-item list-item__second">{{ text_content_2_5 }}</li>
                              </ol>
                          </div>
                      </div>
                  </div>
              </section>

            </div>

        </div>
        <div class="content__footer">
            {{ footer }}
        </div>
    </div>
</div>
